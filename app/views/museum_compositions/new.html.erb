<h1>New Composition</h1>

  <%= form_with url: "/museums/#{@museum.id}/compositions", method: :post, local: true do |form| %>
    <%= form.label :name %>
    <%= form.text_field :name %><br>
    <%= form.label :artist %>
    <%= form.text_field :artist %><br>
    <%= form.label :on_display %>
    <%= form.check_box :on_display %><br>
    <%= form.label :year_made %>
    <%= form.number_field :year_made %><br>
    <%= form.submit 'Create Composition' %>
  <% end %>
